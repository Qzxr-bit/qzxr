#include <iostream>
using namespace std;
int main()
{
    int n;
    cout << "n =";
    cin >> n;
    for (int i = 1; i <=n; i++) // для j от 1 до n с шагом 1
    cout << i << "\t"; // выводить на экран значение j
    return 0;
}

#include <iostream>
using namespace std;
int main()
{
    int n, i = 1;
    cout << "n =";
    cin >> n;
    do // выводим на экран j, а затем увеличиваем
    cout << i++<< "\t"; // ее значение на единицу
    while (i<= n); // до тех пор пока j меньше или равна n
    return 0;
}

